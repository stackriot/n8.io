{
  "private": true,
  "name": "n8.io",
  "description": "@TooTallNate's personal blog - powered by Ghost, running on Now",
  "scripts": {
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/TooTallNate/n8.io.git"
  },
  "keywords": [
    "now",
    "deploy",
    "zeit",
    "ghost",
    "blog"
  ],
  "author": "Nathan Rajlich <nate@zeit.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TooTallNate/n8.io/issues"
  },
  "homepage": "https://github.com/TooTallNate/n8.io#readme",
  "engines": {
    "node": "6"
  },
  "dependencies": {
    "ghost": "0.11.9"
  },
  "now": {
    "alias": "n8.io",
    "env": {
      "NODE_ENV": "production",
      "MAILGUN_USERNAME": "@n8-io-mailgun-username",
      "MAILGUN_PASSWORD": "@n8-io-mailgun-password"
    }
  }
}
